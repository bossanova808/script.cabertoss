msgctxt "#32000"
msgid "General"
msgstr "Allmänt"

msgctxt "#32001"
msgid "Toss log files to..."
msgstr "Kasta loggfiler till..."

msgctxt "#32023"
msgid "Windows crashlogs are not supported, sorry"
msgstr "Kraschloggar för Windows stöds inte, tyvärr"

msgctxt "#32024"
msgid "Android crashlogs are not supported, sorry"
msgstr "Kraschloggar för Android stöds inte, tyvärr"

msgctxt "#32025"
msgid "No log files found to copy ?!"
msgstr "Inga loggfiler hittades att kopiera!"

msgctxt "#32026"
msgid "Error copying logs"
msgstr "Fel vid kopiering av loggar"

msgctxt "#32027"
msgid "No destination path set in the addon settings!"
msgstr "Ingen målsökväg inställd i tilläggsinställningarna!"

msgctxt "#32028"
msgid "Log files copied"
msgstr "Loggfiler kopierade"

msgctxt "#32029"
msgid "Something went wrong, (ironically) check your logs!"
msgstr "Något gick fel, (ironiskt nog) kontrollera dina loggar!"

msgctxt "#32030"
msgid "Working..."
msgstr "Jobbar…"

msgctxt "#32031"
msgid "Error making directory to copy log files to!"
msgstr "Fel vid skapande av katalog för att kopiera loggfiler till!"

msgctxt "#32032"
msgid "Max age of crashlogs to copy (days)"
msgstr "Max ålder för kraschloggar som ska kopieras (dagar)"
